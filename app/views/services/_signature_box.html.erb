<%= image_tag((f.object.send(field).present? ? f.object.send(field) : "data:image/png;base64,"), :class => "bigger_preview") %>
<%= button_tag "Sign", :class => "signature_button" %>
<%= button_tag "Clear", :class => "signature_clear_button" %>
<div class="signature_wrapper">
  <div class="signature_box" id="<%= "#{f.object_name.underscore.gsub(/[\]\[]+/, "_")}_#{field}" %>_sig" data-init="false"></div>
  <%= button_tag "Reload", :class => "signature_reload_button" %>
  <%= button_tag "Done", :class => "signature_done_button" %>
</div>
